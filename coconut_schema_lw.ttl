:containsSugar rdf:type owl:DatatypeProperty ;
       rdfs:domain :TopologicalDescriptor ;
       rdfs:range xsd:boolean ;
       rdfs:comment"Utilizing the Sugar Removal Utility to check for the presence of circular or linear sugars."@en ;
       rdfs:label "Contains Sugar"@en .

:containsRingSugars rdf:type owl:DatatypeProperty ;
       rdfs:domain :TopologicalDescriptor ;
       rdfs:range xsd:boolean ;
       rdfs:comment"Utilizing the Sugar Removal Utility to check for the presence of circular sugars."@en ;
       rdfs:label "Contains Ring Sugars"@en .

:containsLinearSugars rdf:type owl:DatatypeProperty ;
       rdfs:domain :TopologicalDescriptor ;
       rdfs:range xsd:boolean ;
