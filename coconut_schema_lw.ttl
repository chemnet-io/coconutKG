############################
# Data Properties
############################

# Electronic Descriptor
:formalCharge rdf:type owl:DatatypeProperty ;
              rdfs:domain :ElectronicDescriptor ;
              rdfs:range xsd:string ;
              rdfs:comment "The formal charge of the molecule."@en ;
              rdfs:label "Formal Charge"@en .

:hydrogenBondDonors rdf:type owl:DatatypeProperty ;
                    rdfs:domain :ElectronicDescriptor ;
                    rdfs:range xsd:int ;
                    rdfs:comment "The number of Hydrogen Bond Donors in a molecule."@en ;
                    rdfs:label "Hydrogen Bond Donors"@en .

:hydrogenBondDonorsLipinski rdf:type owl:DatatypeProperty ;
                    rdfs:domain :ElectronicDescriptor ;
                    rdfs:range xsd:int ;
                    rdfs:comment "Number of Hydrogen Bond Donors in a molecule using the Lipinski module."@en ;
                    rdfs:label "Hydrogen Bond Donors Lipinski"@en .

:hydrogenBondAcceptors rdf:type owl:DatatypeProperty ;
                    rdfs:domain :ElectronicDescriptor ;
                    rdfs:range xsd:int ;
                    rdfs:comment "The number of Hydrogen Bond Acceptors in a molecule."@en ;
                    rdfs:label "Hydrogen Bond Acceptors"@en .

:hydrogenBondAcceptorsLipinski rdf:type owl:DatatypeProperty ;
                    rdfs:domain :ElectronicDescriptor ;
                    rdfs:range xsd:int ;
                    rdfs:comment "Number of Hydrogen Bond Acceptors in a molecule using the Lipinski module."@en ;
                    rdfs:label "Hydrogen Bond Acceptors Lipinski"@en .

:AlogP rdf:type owl:DatatypeProperty ;
       rdfs:domain :ElectronicDescriptor ;
       rdfs:range xsd:decimal ;
       rdfs:comment "The logarithm of the partition between n-octanol and water (logP). AlogP is used to estimate the lipophilicity of a molecule, which is an important factor in drug absorption and distribution."@en ;
       rdfs:label "AlogP"@en .

:LipinskiRuleOfFiveViolations rdf:type owl:DatatypeProperty ;
       rdfs:domain :ElectronicDescriptor ;
       rdfs:range xsd:int ;
       rdfs:comment "A custom function to determine the number of Lipinski's rule violations for a given molecule."@en ;
       rdfs:label "Lipinski Rule Of Five Violations"@en .

:qedDrugLikeliness rdf:type owl:DatatypeProperty ;
       rdfs:domain :ElectronicDescriptor ;
       rdfs:range xsd:decimal ;
       rdfs:comment "Quantative estimation of drug-likeness (q weighted sum)."@en ;
       rdfs:label "Qed Drug Likeliness"@en .

# Topological Descriptors
:topologicalPolarSurfaceArea rdf:type owl:DatatypeProperty ;
       rdfs:domain :TopologicalDescriptor ;
       rdfs:range xsd:decimal ;
       rdfs:comment"Topological Polar Surface Area (TPSA) of a molecule. TPSA is an important descriptor in medicinal chemistry and drug design, as it provides an estimate of the polar surface area of a molecule."@en ;
       rdfs:label "Topological Polar Surface Area"@en .

:rotatableBondCount rdf:type owl:DatatypeProperty ;
       rdfs:domain :TopologicalDescriptor ;
       rdfs:range xsd:int ;
       rdfs:comment"The number of rotatable bonds in a molecule."@en ;
       rdfs:label "Rotatable Bond Count"@en .

:aromaticRingCount rdf:type owl:DatatypeProperty ;
       rdfs:domain :TopologicalDescriptor ;
       rdfs:range xsd:int ;
       rdfs:comment"The number of aromatic rings in a molecule."@en ;
       rdfs:label "Aromatic Ring Count"@en .

:numberOfMinimalRings rdf:type owl:DatatypeProperty ;
       rdfs:domain :TopologicalDescriptor ;
       rdfs:range xsd:int ;
       rdfs:comment"The number rings present in a molecule."@en ;
       rdfs:label "Number Of Minimal Rings"@en .

:murkoFramework rdf:type owl:DatatypeProperty ;
       rdfs:domain :TopologicalDescriptor ;
       rdfs:range xsd:string ;
       rdfs:comment"Custom function based on CDK to get the Murko Framework."@en ;
       rdfs:label "Murko Framework"@en .

