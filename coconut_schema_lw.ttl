:chemicalClass rdf:type owl:DatatypeProperty ;
       rdfs:domain :ConstitutionalDescriptor ;
       rdfs:range xsd:string ;
       rdfs:comment"Chemical Class generated using ClassyFire."@en ;
       rdfs:label "Chemical Class"@en .

:chemicalSubClass rdf:type owl:DatatypeProperty ;
       rdfs:domain :ConstitutionalDescriptor ;
       rdfs:range xsd:string ;
       rdfs:comment"Chemical Sub Class generated using ClassyFire."@en ;
       rdfs:label "Chemical Sub Class"@en .

:chemicalSuperClass rdf:type owl:DatatypeProperty ;
       rdfs:domain :ConstitutionalDescriptor ;
       rdfs:range xsd:string ;
       rdfs:comment"Chemical Super Class generated using ClassyFire."@en ;
       rdfs:label "Chemical Super Class"@en .

:directParentClassification rdf:type owl:DatatypeProperty ;
       rdfs:domain :ConstitutionalDescriptor ;
       rdfs:range xsd:string ;
       rdfs:comment"Direct Parent Classification generated using ClassyFire."@en ;
       rdfs:label "Direct Parent Classification"@en .

:fractioncsp3 rdf:type owl:DatatypeProperty ;
       rdfs:domain :ConstitutionalDescriptor ;
       rdfs:range xsd:decimal ;
       rdfs:comment"The fraction of C atoms that are SP3 hybridized."@en ;
       rdfs:label "Fraction CSP3"@en .

