############################
# Classes
############################

:Citation rdf:type owl:Class ;
	rdfs:comment "An article, book or document with a DOI in which a molecule or a collection was mentioned."@en ;
	rdfs:label "Citation"@en .

:Collection rdf:type owl:Class ;
	rdfs:comment "Groups of molecules are organized within collections, each linked to metadata such as titles, descriptions, and related jobs."@en ;
	rdfs:label "Collection"@en .

:Entry rdf:type owl:Class ;
	rdfs:comment ""@en ;
	rdfs:label "Entry"@en .

:GeoLocation rdf:type owl:Class ;
	rdfs:comment "A geographical location where an organism is reported to be found."@en ;
	rdfs:label "GeoLocation"@en .

:Organism rdf:type owl:Class ;
	rdfs:comment "A living thing that has an organized structure. The biological origin of a given molecule."@en ;
	rdfs:label "Organism"@en .

:Properties rdf:type owl:Class ;
	rdfs:comment "The chemical and biological properties of a molecule."@en ;
	rdfs:label "Properties"@en .

:Molecule rdf:type owl:Class ;
	rdfs:comment "A group of atoms bonded together, representing the smallest fundamental unit of a chemical compound that can take part in a chemical reaction."@en ;
	rdfs:label "Molecule"@en .

# Commented out due to it not being in the dataset yet
#:SampleLocation rdf:type owl:Class ;
#	rdfs:comment "Sample location within the organism from where the sample of the compound is taken."@en ;
#	rdfs:label "SampleLocation"@en .

:Identifier rdf:type owl:Class ;
	rdfs:comment "Unique Identifiers for a given molecule."@en ;
	rdfs:label "Identifier"@en .

:Classification rdf:type owl:Class ;
	rdfs:comment "Attributes related to the classification molecules."@en ;
	rdfs:label "Classification"@en .

:DescriptiveInformation rdf:type owl:Class ;
	rdfs:comment "Descriptive information related to synonyms, comments, or other textual metadata of a molecule."@en ;
	rdfs:label "Descriptive Information"@en .

:Counts rdf:type owl:Class ;
	rdfs:comment "Attributes counting variants, collections, and other related data of a molecule."@en ;
	rdfs:label "Counts"@en .

:Status rdf:type owl:Class ;
	rdfs:comment "Current state and activity of a molecule."@en ;
	rdfs:label "Status"@en .

:Descriptor rdf:type owl:Class ;
	rdfs:comment "Descriptors of a molecule."@en ;
	rdfs:label "Descriptor"@en .

:GeometricalDescriptor rdf:type owl:Class ;
  rdfs:subClassOf :Descriptor ;
  rdfs:comment "Geometrical descriptors of a chemical compound."@en ;
	rdfs:label "Geometrical Descriptor"@en .

:TopologicalDescriptor rdf:type owl:Class ;
  rdfs:subClassOf :Descriptor ;
  rdfs:comment ""@en ;
	rdfs:label "Topological Descriptor"@en .

:ElectronicDescriptor rdf:type owl:Class ;
  rdfs:subClassOf :Descriptor ;
  rdfs:comment "Electronic descriptors of a chemical compound."@en ;
	rdfs:label "Electronic Descriptor"@en .

:ConstitutionalDescriptor rdf:type owl:Class ;
  rdfs:subClassOf :Descriptor ;
  rdfs:comment "Constitutional descriptors of a chemical compound."@en ;
	rdfs:label "Constitutional Descriptor"@en .

